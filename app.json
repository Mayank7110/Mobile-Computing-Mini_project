{
  "expo": {
    "name": "Ryde",
    "slug": "Ryde",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash":
      {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#2F80ED"
      },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
      ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
