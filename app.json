{
  "expo": {
    "name": "Ryde",
    "slug": "Ryde",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "permissions": ["location"],
    "splash":
      {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#2F80ED"
      },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCrlODzQIrmQ3vs7SxCRkGD1qrt4_XXsZ4"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router", {"origin":"https://ryde.com/"}
    ]
      ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
